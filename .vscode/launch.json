    "version": "0.2.0",
    "configurations": [
        {
            "name": "LOUSY",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/bin_rgen/x86_64/lousy/bin/rgen.exe",
            "args": [
                "--unit", "0",
                "--width", "1024",
                "--height", "1024",
                "--log", "0",
                "--data", "data/"
             ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "LOUSY",
            "miDebuggerPath": "C:\\msys64\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
