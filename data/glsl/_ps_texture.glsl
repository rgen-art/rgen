uniform highp sampler2D u_t2Texture_0;
uniform highp vec4 u_v4Color;

//-----------------------------------------------------------------------------
//-----------------------------------------------------------------------------
varying highp vec4 v_v4Texcoord;

//-----------------------------------------------------------------------------
//-----------------------------------------------------------------------------
void main()
{ o_v4Color = texture2D(u_t2Texture_0, v_v4Texcoord.xy) * u_v4Color; }

